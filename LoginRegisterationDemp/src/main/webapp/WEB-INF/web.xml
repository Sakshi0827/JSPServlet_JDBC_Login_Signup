  	<url-pattern>/editEmp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateEmployee</servlet-name>
  	<url-pattern>/updateEmp</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
